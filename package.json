{
  "name": "amana-wallet",
  "version": "1.0.0",
  "description": "The front-end wallet for AMANA",
  "main": "index.js",
  "scripts": {
    "rebuild": "electron-rebuild -o leveldown",
    "pack:mac": "electron-builder --mac",
    "pack:windows": "electron-builder --win",
    "pack:linux": "electron-builder --linux",
    "build": "rm -rf webpack-dist && webpack",
    "start": "electron webpack-dist/index.js",
    "dev": "npm run build && npm start",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "asar:extract": "asar extract dist/mac/amana-wallet.app/Contents/Resources/app.asar extracted-asar",
    "fullbuild": "rm -rf dist && rm -rf webpack-dist && rm -rf extracted-asar && npm run build && npm run pack:mac && npm run asar:extract"
  },
  "keywords": [],
  "author": "",
  "build": {
    "appId": "amanawallet",
    "files": [
      "webpack-dist/**/*",
      "index.js"
    ],
    "mac": {
      "target": {
        "target": "default",
        "arch": "universal"
      },
      "identity": null
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@railgun-community/shared-models": "6.3.0",
    "@railgun-community/wallet": "7.0.1",
    "@reduxjs/toolkit": "2.1.0",
    "async-mutex": "0.5.0",
    "bip39": "3.1.0",
    "buffer": "6.0.3",
    "classnames": "2.5.1",
    "eleventh": "1.1.0",
    "ethers": "6.7.10",
    "level": "8.0.1",
    "leveldown": "6.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "react-redux": "9.1.0",
    "react-toastify": "10.0.4",
    "snarkjs": "0.7.3"
  },
  "devDependencies": {
    "@electron/rebuild": "3.6.0",
    "@types/leveldown": "4.0.6",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/snarkjs": "0.7.8",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "asar": "3.2.0",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "6.9.1",
    "electron": "29.1.4",
    "electron-builder": "24.13.3",
    "eslint": "8.56.0",
    "eslint-config-principled": "1.1.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.6.0",
    "mini-css-extract-plugin": "2.7.7",
    "sass": "1.70.0",
    "sass-loader": "14.0.0",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "webpack": "5.90.0",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}
